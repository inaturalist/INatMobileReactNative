type UserLocation = {
  latitude: number,
  longitude: number,
  positional_accuracy: number

}
const fetchUserLocation = async ( ): Promise<UserLocation> => new Promise( resolve => {
  setTimeout( ( ) => resolve( {
    latitude: 37.7749,
    longitude: -122.4194,
    positional_accuracy: 5
  } ), 1000 );
} );

export default fetchUserLocation;
