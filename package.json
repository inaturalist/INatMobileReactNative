{
  "name": "inaturalistreactnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 13'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "manifest": "fbt-manifest --src src/ --enum-manifest i18n/fbt/.enum_manifest.json --src-manifest i18n/fbt/.src_manifest.json",
    "collect-fbts": "fbt-collect --hash-module fb-tiger-hash --react-native-mode --manifest < i18n/fbt/.src_manifest.json > i18n/fbt/.source_strings.json",
    "translate-fbts": "fbt-translate --jenkins --source-strings i18n/fbt/.source_strings.json --translations i18n/fbt/translationScriptInput/*.json > i18n/fbt/translatedFbts.json",
    "generate-android-fbt": "babel-node i18n/scripts/generate-android-localizables-executor.js"
  },
  "dependencies": {
    "@fbtjs/default-collection-transform": "^0.0.3",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.4",
    "babel-plugin-fbt": "^0.20.3",
    "babel-plugin-fbt-runtime": "^0.9.18",
    "fbt": "^0.16.6",
    "inaturalistjs": "github:inaturalist/inaturalistjs",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-fbt": "^0.0.5",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-maps": "^0.29.3",
    "react-native-reanimated": "^2.2.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "realm": "^10.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.16.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^7.2.0",
    "babel-jest": "^26.6.3",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "^6.0.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "fb-tiger-hash": "^0.1.6",
    "flow-bin": "^0.149.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-native-accessibility-engine": "^0.6.0",
    "react-native-clean-project": "^3.6.7",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "shelljs": "^0.8.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "automock": false,
    "resetMocks": false,
    "verbose": true
  }
}
